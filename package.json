{
  "name": "bitbadges-crx",
  "displayName": "bitbadges-crx",
  "version": "0.1.3",
  "author": "Trevor Miller",
  "description": "This is a browser extension that allows users to view BitBadges portfolio insights.",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "react",
    "vite",
    "create-chrome-ext"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "fmt": "prettier --write '**/*.{tsx,ts,json,css,scss,md}'",
    "zip": "npm run build && node src/zip.js"
  },
  "dependencies": {
    "antd": "^4.24.15",
    "bitbadgesjs-sdk": "^0.3.137",
    "react": "^18.2.0",
    "react-blockies": "^1.4.1",
